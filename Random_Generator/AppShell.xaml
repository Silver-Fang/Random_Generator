<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="Random_Generator.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:local="clr-namespace:Random_Generator" Navigated="Shell_Navigated">
	<ShellContent Title="数值" ContentTemplate="{DataTemplate local:RandomNumber}" Route="RandomNumber" Icon="number.png"/>
	<ShellContent Title="密码" ContentTemplate="{DataTemplate local:RandomPassword}" Route="RandomPassword" Icon="password.png"/>
	<ShellContent Title="汉字" ContentTemplate="{DataTemplate local:RandomChinese}" Route="RandomChinese" Icon="chinese.png"/>
	<ShellContent Title="乱序" ContentTemplate="{DataTemplate local:RandomShuffle}" Route="RandomShuffle" Icon="shuffle.png"/>
</Shell>
